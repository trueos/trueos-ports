pipeline {
  agent { label 'JenkinsJail' }

  environment {
    GH_ORG = 'trueos'
    GH_REPO = 'trueos-ports'
    GH_BUILD_BRANCH = 'trueos-master'
  }

  stages {
    stage('Checkout') {
      steps {
        checkout scm
      }
    }

    stage('INDEX Test') {
      steps {
        build 'TrueOS - Stable - Complete'
      }
    }
  }
}
