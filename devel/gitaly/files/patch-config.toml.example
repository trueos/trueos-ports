--- config.toml.example.orig	2018-04-12 00:44:37 UTC
+++ config.toml.example
@@ -1,9 +1,9 @@
 # Example Gitaly configuration file
 
-socket_path = "/home/git/gitlab/tmp/sockets/private/gitaly.socket"
